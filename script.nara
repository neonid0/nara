// Variables
val name = "Nara"
val version = 0.2

// Functions
fn greet(name) {
  f"Hello, {name}!"
}

// Lists and iteration
val numbers = range(10)
for num in numbers {
  print(f"Number: {num}")
}

// Control flow
fn factorial(n) {
  if n <= 1 {
    1
  } else {
    n * factorial(n - 1)
  }
}

// String interpolation
val message = f"Factorial of 5 is {factorial(5)}"
print(message)
